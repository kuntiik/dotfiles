{
    "[python]": {
        "editor.formatOnType": true,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "ms-python.black-formatter",
      "editor.codeActionsOnSave": {
          "source.organizeImports": "explicit"
      },
    },
    // "python.defaultInterpreterPath": "/Users/kuntl/msd/table_parsing/table-transformer/.venv/bin/python",
    // "python.defaultInterpreterPath": "/Users/kuntl/msd/crweg-irap-medagent/backend/app/.venv/bin/python",
    // "python.defaultInterpreterPath": "/Users/kuntl/msd/rag_evaluation/app/.venv/bin/python",
    // "python.defaultInterpreterPath": "/Users/kuntl/msd/rag_evaluation/app/.venv/bin/python",
    "python.defaultInterpreterPath": "/Users/kuntl/msd/rag_poc_luis/backend/.venv/bin/python",
    // "python.defaultInterpreterPath": "/Users/kuntl/msd/content-catalyst/projects/mlr_backend/.venv/bin/python",
     "python.envFile": "${workspaceFolder}/.env",
    "workbench.colorTheme": "One Dark Pro",
    "python.experiments.optOutFrom": [
      "pythonTestAdapter"
    ],
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "jupyter.debugJustMyCode": false,
    // "find-it-faster.general.useTer"

    // VIM settings
    "vim.useSystemClipboard": true,
    "vim.incsearch": true,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
    {
        "before": ["<leader>", "l"],
        "commands": ["workbench.action.focusRightGroup"],
    },
    {
        "before": ["<leader>", "h"],
        "commands": ["workbench.action.focusLeftGroup"],
    },
    {
        "before": ["<leader>", "j"],
        "commands": ["workbench.action.focuesBellowGroup"],
    },
    {
        "before": ["<leader>", "k"],
        "commands": ["workbench.action.focusAboveGroup"],
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.view.explorer"]
    },
    {
        "before": ["g", "s"],
        "commands": ["workbench.action.gotoSymbol"]
    },
    {
        "before": ["g", "a"],
        "commands": ["workbench.action.showAllSymbols"]
    },
    {
        "before": ["<leader>", "m"],
        "commands": ["editor.action.toggleMinimap"]
    },
    {
        "before": ["<leader>", "="],
        "commands": ["editor.action.formatDocument"]
    },
    {
        "before": ["<leader>", "-"],
        "commands": ["editor.action.organizeImports"]
    },
    {
      "before": ["g", "p", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["g", "h"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "p", "i"],
      "commands": ["editor.action.peekImplementation"]
    },
    {
      "before": ["g", "q"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["g", "t"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["g", "p", "t"],
      "commands": ["editor.action.peekTypeDefinition"]
    },
{
      "before": ["leader", "f", "r"],
      "commands": ["find-it-faster.findWithinFiles"]
    },
{
      "before": ["leader", "f", "f"],
      "commands": ["find-it-faster.findFiles"]
    },
],
"editor.minimap.enabled": false,
"git.autofetch": true,
"jupyter.askForKernelRestart": false,
"python.testing.pytestEnabled": true,
"redhat.telemetry.enabled": true,
"xml.symbols.maxItemsComputed": 50000,
"jupyter.widgetScriptSources": [
    "jsdelivr.com",
    "unpkg.com"
],
"explorer.confirmDelete": false,
"github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
},
"terminal.integrated.env.osx": {},
"remote.SSH.remotePlatform": {
  "sandbox.z3wq9q.csb": "linux",
  "*.gitpod.io": "linux"
},
"editor.defaultFormatter": "ms-python.black-formatter",
"explorer.fileNesting.patterns": {
  "*.ts": "${capture}.js",
  "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
  "*.jsx": "${capture}.js",
  "*.tsx": "${capture}.ts",
  "tsconfig.json": "tsconfig.*.json",
  "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
  "*.sqlite": "${capture}.${extname}-*",
  "*.db": "${capture}.${extname}-*",
  "*.sqlite3": "${capture}.${extname}-*",
  "*.db3": "${capture}.${extname}-*",
  "*.sdb": "${capture}.${extname}-*",
  "*.s3db": "${capture}.${extname}-*"
},
"cSpell.userWords": [
  "irap",
  "langchain",
  "textractor",
  "tika"
],
"[json]": {
  "editor.defaultFormatter": "vscode.json-language-features"
},
"notebook.lineNumbers": "on",
"githubIssues.queries": [
  {
    "label": "My Issues",
    "query": "is:open assignee:${user} repo:${owner}/${repository}",
    "groupBy": [
      "milestone"
    ]
  },
  {
    "label": "Created Issues",
    "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
  },
  {
    "label": "Recent Issues",
    "query": "state:open repo:${owner}/${repository} sort:updated-desc"
  }
],
"explorer.confirmDragAndDrop": false,
"remote.SSH.defaultExtensions": [
  "gitpod.gitpod-remote-ssh"
],
"dev.containers.defaultExtensions": [
  "gitpod.gitpod-remote-ssh"
],
"isort.importStrategy": "fromEnvironment",
"gitlens.views.scm.grouped.views": {
  "commits": true,
  "branches": true,
  "remotes": true,
  "stashes": true,
  "tags": true,
  "worktrees": true,
  "contributors": true,
  "repositories": false,
  "searchAndCompare": true,
  "launchpad": false
},
"terminal.integrated.defaultProfile.osx": "bash",
"[html]": {
  "editor.defaultFormatter": "vscode.html-language-features"
},
"http.experimental.systemCertificatesV2": true
}